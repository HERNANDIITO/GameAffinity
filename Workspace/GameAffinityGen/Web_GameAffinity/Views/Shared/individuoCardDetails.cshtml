@model GameAffinityGen.ApplicationCore.EN.GameAffinity.IndividuoEN

<a class="col-6 col-md-6 col-lg-3 videogame" asp-action="Details" asp-controller="Individuo" asp-route-id="@Model.Id">
    <img src=@Model.Img class="videogameImage" onerror="this.src='/Images/missing_img.jpg';" />
    <span class="videogameName">
        @Model.Nombre
    </span>
</a>
