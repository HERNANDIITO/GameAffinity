@model GameAffinityGen.ApplicationCore.EN.GameAffinity.EmpresaEN

<a class="col-4 col-md-3 col-lg-2 videogame" asp-action="Details" asp-controller="Empresa" asp-route-id="@Model.Id">
    <section class="videogameScore">
        <span class="score">@Html.DisplayFor(modelItem => Model.Nota)</span>
    </section>
    <img src=@Model.Img class="videogameImage" onerror="this.src='Images/missing_img.jpg';" />
    <span class="videogameName">
        @Html.DisplayFor(modelItem => Model.Nombre)
    </span>
</a>
