<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="GameAffinityGen.Infraestructure"
		                   namespace="GameAffinityGen.Infraestructure.EN.GameAffinity">
		    
		    
			
				<class name="ResenyaNH" table="Resenya"> 
				  	
						<id name="Id" column="idResenya" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Titulo" type="string"> 
						<column name="titulo"  not-null="true"/>
					</property>
					<property name="Texto" type="string"> 
						<column name="texto"  not-null="true"/>
					</property>
					<property name="Likes" type="integer"> 
						<column name="likes"  not-null="true"/>
					</property>
					<property name="Dislikes" type="integer"> 
						<column name="dislikes"  not-null="true"/>
					</property>
			
			
			
			
			
			
			
		  			
						<many-to-one name="Escribe" class="GameAffinityGen.Infraestructure.EN.GameAffinity.RegistradoNH" >	
								<column name="FK_idRegistrado_idRegistrado_0"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Resenyado" class="GameAffinityGen.Infraestructure.EN.GameAffinity.VideojuegoNH" >	
								<column name="FK_idVideojuego_idVideojuego"/> 
						</many-to-one>
											 					
			
		  			
						<bag name="Mg" >
							<key>
							
								
								<column name="FK_idResenya_idResenya"/>
					      	</key>
							<one-to-many class="GameAffinityGen.Infraestructure.EN.GameAffinity.LikeNH"/>
					    </bag>
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ResenyaNHReadAllHQL">from ResenyaNH</query>
		</hibernate-mapping>
	